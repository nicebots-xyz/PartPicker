{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
    "config:recommended"
  ],
  "baseBranches": [
    "dev"
  ],
  "labels": [
    "deps"
  ],
  "commitMessagePrefix": "⬆️",
  "commitMessageAction": "Upgrade",
  "packageRules": [
    {
      "updateTypes": [
        "pin"
      ],
      "commitMessagePrefix": "📌",
      "commitMessageAction": "Pin"
    },
    {
      "updateTypes": [
        "rollback"
      ],
      "commitMessagePrefix": "⬇️",
      "commitMessageAction": "Downgrade"
    }
  ]
}



